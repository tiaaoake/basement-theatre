{.repeated section items}
{.if customContent.isFeatured}

{.or}
<div class="sqs-col sqs-col-3">
  <div class="coming-soon-thumb sqs-row" id="card1">
    <div class="content-fill" id="post-image">
      <img src="{bannerImg}{assetUrl}">
    </div>
    <h2><a href="{fullUrl}">{title}</a> | {customContent.times}</h2>
    <p>{customContent.dates}</p>
  </div>

</div>{.end}{.end}
